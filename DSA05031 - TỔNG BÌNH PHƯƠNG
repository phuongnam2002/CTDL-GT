template<class X,class Y>
    void minimize(X &x,const Y &y) {
        if (x>y) x=y;
    }
template<class X,class Y>
    void maximize(X &x,const Y &y) {
        if (x<y) x=y;
    }
template<class T>
    T Abs(const T &x) {
        return (x<0?-x:x);
    }
int n,a[500],f[N];
void xuly()
{
    cin>>n;
    int m=0;
    f1(i,n) f[i]=i;
    f1(i,n)
    {
        f1(j,sqrt(i)) f[i]=min(f[i],f[i-j*j]+1);
    }
    cout<<f[n];el;
}
int main()
{
  ios::sync_with_stdio(false);cin.tie(0);cout.tie(0);
